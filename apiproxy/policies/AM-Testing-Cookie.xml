<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM-Testing-Cookie">
    <DisplayName>AM-Testing-Cookie</DisplayName>
    <Properties/>
    <Set>
        <StatusCode>302</StatusCode>
        <Headers>
            <Header name="Set-Cookie">testing_cookie={messageid}; Secure; HttpOnly; Path=/; SameSite=None;</Header>
            <!--<Header name="Location">http://localhost:3000</Header>-->
            <Header name="Location">http://localhost:3000</Header>
            <!--<Header name="Location">https://reqbin.com/</Header>-->
        </Headers>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>